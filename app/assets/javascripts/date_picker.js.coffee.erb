$ ->
  $(".datepicker").datetimepicker(
    timepicker: false,
    format: "Y/m/d",
    defaultDate: '<%= EventConfiguration.singleton.start_date.try(:strftime, "%Y/%m/%d") %>'
    )

  $(".datetimepicker").datetimepicker({
    defaultDate: '<%= EventConfiguration.singleton.start_date.try(:strftime, "%Y/%m/%d") %>'
  })
