<h1>All Registrants</h1>

<table class="sortable">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Country</th>
    </tr>
  </thead>
  <tbody>
    <% @registrants.each do |registrant| %>
      <tr>
        <td><%= registrant.bib_number %></td>
        <td><%= registrant %></td>
        <td><%= registrant.country_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
