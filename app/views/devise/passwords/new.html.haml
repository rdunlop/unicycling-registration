%h2= t(".title")
= form_for(resource, :as => resource_name, :url => password_path(resource_name), :html => { :method => :post, class: "no_dirty_check" }) do |f|
  = devise_error_messages!
  %div
    = f.label :email
    %br
    = f.email_field :email
  %div= f.submit t(".action"), :class => "focus_button"
= render "devise/shared/links"
