%table
  %thead
    %tr
      %th= t(".entry")
      %th= t(".number_of_registrants")
  %tbody
    - age_group_type.age_group_entries.each do |ag_entry|
      - competitors_in_age_group = ag_entry.number_matching_registrant(age_group_data)
      %tr
        %td= ag_entry
        %td= competitors_in_age_group
