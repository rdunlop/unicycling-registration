- cache [import_result, Registrant, import_result.competition] do
  %tr
    %td= import_result_counter
    %td= import_result.bib_number
    = render partial: "matching_registrant_cell", locals: { import_result: import_result }
    %td= import_result.disqualified? ? "Yes" : ""
    %td= import_result.number_of_penalties > 0 ? "yes" : "no"
    - displayer = import_result.competition.results_displayer
    - displayer.result_data(import_result).each do |entry|
      %td= entry

    %td= link_to t("edit"), edit_import_result_path(import_result)
    %td= link_to t("delete"), import_result, method: :delete, data: { confirm: t("are_you_sure") }
